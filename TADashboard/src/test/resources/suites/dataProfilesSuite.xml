<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="TestSuite" thread-count="1" parallel="false" >
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="utils.common.TestListener" />
    </listeners>
    <test name="Run Chrome Local">
        <parameter name="Browser" value="chrome.local"/>
        <classes>
            <class name="tests.data_profiles.PositiveDPTests"/>
            <class name="tests.general_settings.NegativeDPGeneralSettingsTest"/>
        </classes>
    </test>
    <test name="Run Firefox Local">
        <parameter name="Browser" value="firefox.local"/>
        <classes>           
            <class name="tests.data_profiles.PositiveDPTests"/>
            <class name="tests.general_settings.NegativeDPGeneralSettingsTest"/>
        </classes>
    </test>
</suite>